{
  "pipeline": {
    "roleArn": "arn:aws:iam::account-id:role/my-AWS-CodePipeline-service-role-name",
    "stages": [
      {
        "name": "Source",
        "actions": [
          {
            "inputArtifacts": [],
            "name": "Source1",
            "actionTypeId": {
              "category": "Source",
              "owner": "AWS",
              "version": "1",
              "provider": "CodeCommit"
            },
            "configuration": {
              "RepositoryName": "test-repo",
              "BranchName": "main"
            },
            "runOrder": 1
          }
        ]
      },
      {
        "name": "Build",
        "actions": [
          {
            "inputArtifacts": [
              {
                "name": "Source1"
              }
            ],
            "name": "Build",
            "actionTypeId": {
              "category": "Build",
              "owner": "AWS",
              "version": "1",
              "provider": "CodeBuild"
            },
            "configuration": {
              "ProjectName": "test-ga-build",
              "PrimarySource": "Source1",
              "BatchEnabled": "true",
              "EnvironmentVariables": "[{\"name\":\"TF_COMMAND\",\"value\":\"plan\",\"type\":\"PLAINTEXT\"},{\"name\":\"TF_OPTIONS\",\"value\":\"-var-file=./environments/dev/params.tfvars\",\"type\":\"PLAINTEXT\"},{\"name\":\"ENVIRONMENT\",\"value\":\"dev\",\"type\":\"PLAINTEXT\"}]"
            },
            "runOrder": 1
          }
        ]
      }
    ],
    "artifactStore": {
      "type": "S3",
      "location": "mazy-s3-bucket"
    },
    "name": "cp-batch-test2",
    "version": 1
  }
}